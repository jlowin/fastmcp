{
  "name": "http-server",
  "version": "1.0.0",
  "entrypoint": "echo.py:mcp",
  "description": "HTTP echo server with dependencies",
  "transport": "http",
  "host": "0.0.0.0",
  "port": 8080,
  "path": "/mcp",
  "dependencies": [
    "httpx>=0.24.0",
    "pydantic>=2.0.0"
  ],
  "python_version": ">=3.10",
  "log_level": "DEBUG",
  "env": {
    "SERVER_MODE": "production",
    "MAX_CONNECTIONS": "100"
  }
}